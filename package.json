{
  "name": "track-your-appeal-frontend",
  "description": "Track your appeal",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=. node server.js",
    "test": "cross-env NODE_PATH=. SSCS_MOCK_DATA=true PORT=3021 mocha test/unit/* --reporter spec",
    "coverage": "cross-env NODE_PATH=. SSCS_MOCK_DATA=true istanbul cover _mocha --dir test/coverage test/unit/*",
    "dev": "cross-env NODE_PATH=. LOG_OUTPUT=multi grunt",
    "setup": "cross-env NODE_PATH=. grunt generate-assets",
    "smoke-test": "cross-env NODE_PATH=. codeceptjs run test/smoke/ --steps",
    "pa11y": "cross-env NODE_PATH=. SSCS_MOCK_DATA=true LOG_LEVEL=OFF mocha test/accessibility -r test/chai-sinon.js --reporter spec --recursive --timeout 10000",
    "eslint": "cross-env NODE_PATH=. eslint .",
    "security-checks": "cross-env NODE_PATH=. grunt security-checks"
  },
  "repository": {
    "type": "git",
    "url": "git@10.196.60.5:SSCS/track-your-appeal-frontend.git"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cross-env": "^3.2.3",
    "express": "4.14.0",
    "express-nunjucks": "2.0.2",
    "font-awesome": "4.7.0",
    "govuk-elements-sass": "2.2.1",
    "govuk_frontend_toolkit": "^5.0.1",
    "govuk_template_jinja": "^0.19.0",
    "helmet": "^3.4.1",
    "lodash": "4.17.4",
    "log4js": "^1.1.0",
    "moment": "2.17.1",
    "nunjucks": "2.5.2",
    "serve-favicon": "2.3.0",
    "superagent": "^3.3.2",
    "chai": "^3.5.0",
    "codeceptjs": "^0.4.12",
    "crypto-js": "^3.1.9-1",
    "eslint": "^3.12.1",
    "eslint-plugin-mocha": "^4.7.0",
    "grunt": "1.0.1",
    "grunt-cli": "1.2.0",
    "grunt-concurrent": "2.3.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-env": "^0.4.4",
    "grunt-nodemon": "0.4.2",
    "grunt-nsp": "^2.3.1",
    "grunt-sass": "^1.2.1",
    "grunt-sync": "^0.6.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mochawesome": "^1.5.3",
    "mockery": "2.0.0",
    "nightmare": "^2.8.1",
    "nightmare-upload": "^0.1.1",
    "nsp": "^2.6.2",
    "pa11y": "^4.3.0",
    "request-json": "^0.6.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.1",
    "urlsafe-base64": "^1.0.0"
  }
}
